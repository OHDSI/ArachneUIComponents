{
	"env": {
    	"browser": true
  	},
	"extends": "airbnb",
	"globals": {
		"__APP_TYPE_CENTRAL__": true,
		"__APP_TYPE_NODE__": true
	},
	"settings": {
		"import/resolver": {
			"webpack": { "config": "webpack.config.js" }
		}
	},
	"rules": {
		"import/extensions": ["error", "ignorePackages", {
			"js": "never",
			"jsx": "never"
		}]
	  }
}